(defun repeat (f times)
    (if (<= times 1)
        (f)
        (progn (f) (repeat f (- times 1)))
))

(defun run-gc ()
    (progn
        (define start (systime))
        (repeat 'gc 25)
        (define res (secs-since start))
        (print (list "GC-time (ms):" (* (/ res 25) 1000)))
))
